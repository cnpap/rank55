{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "rank55",
  "private": true,
  "version": "0.0.25",
  "type": "module",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "electron:build-main": "vite build --config vite.electron.config.ts",
    "electron:dev": "npm run electron:build-main && concurrently \"npm run dev\" \"npm run electron:start\"",
    "electron:start": "wait-on http://localhost:3000 && cross-env NODE_ENV=development VITE_DEV_SERVER_URL=http://localhost:3000 electron --enable-logging dist-electron/main.js",
    "electron:build": "rimraf release && npm run build && npm run electron:build-main",
    "electron:preview": "npm run electron:build && electron dist-electron/main.js",
    "dist": "rimraf release && npm run electron:build && electron-builder",
    "dist:win": "rimraf release && npm run electron:build && electron-builder --win",
    "upload": "tsx cmd/cli.ts upload-release"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.858.0",
    "@aws-sdk/lib-storage": "^3.858.0",
    "@aws-sdk/s3-request-presigner": "^3.858.0",
    "@heroicons/vue": "^2.2.0",
    "@tanstack/vue-table": "^8.21.3",
    "@vueuse/core": "^13.7.0",
    "axios-retry": "^4.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "commander": "^14.0.0",
    "dexie": "^4.2.0",
    "electron-log": "^5.4.2",
    "electron-updater": "6.1.7",
    "idb": "^8.0.3",
    "lucide-vue-next": "^0.525.0",
    "mitt": "^3.0.1",
    "pinia": "^3.0.3",
    "pinyin-pro": "^3.27.0",
    "reka-ui": "^2.4.1",
    "tailwind-merge": "^3.3.1",
    "ts-node": "^10.9.2",
    "tw-animate-css": "^1.3.4",
    "vaul-vue": "^0.4.1",
    "vtjump": "^4.0.0",
    "vue": "^3.5.17",
    "vue-draggable-plus": "^0.6.0",
    "vue-router": "^4.5.1",
    "vue-sonner": "^2.0.2"
  },
  "devDependencies": {
    "@babel/types": "^7.27.7",
    "@tailwindcss/vite": "^4.1.11",
    "@types/node": "^24.0.7",
    "@vitejs/plugin-vue": "^6.0.0",
    "concurrently": "^9.2.0",
    "cross-env": "^7.0.3",
    "electron": "^37.2.3",
    "electron-builder": "^26.0.12",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.13",
    "prettier-plugin-vue": "^1.1.6",
    "rimraf": "^6.0.1",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.8.3",
    "vite": "^6.0.0",
    "vitest": "^3.2.4",
    "vue-tsc": "^2.2.10",
    "wait-on": "^8.0.3"
  },
  "build": {
    "appId": "com.yourcompany.frontend",
    "productName": "rank55",
    "directories": {
      "output": "release"
    },
    "files": ["dist/**/*", "dist-electron/**/*", "node_modules/**/*"],
    "extraResources": [
      {
        "from": "public/dynamic",
        "to": "dynamic"
      }
    ],
    "publish": {
      "provider": "generic",
      "url": "https://rank55.com/"
    },
    "win": {
      "requestedExecutionLevel": "requireAdministrator",
      "target": [
        {
          "target": "nsis",
          "arch": ["x64"]
        }
      ],
      "icon": "public/icon.png"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "allowElevation": true,
      "differentialPackage": true
    }
  },
  "packageManager": "yarn@4.9.2"
}
